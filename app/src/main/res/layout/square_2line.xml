<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools" >

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/root"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">

        <TextView
            android:id="@+id/center_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/square_outer"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/center_view2"
            android:layout_width="145dp"
            android:layout_height="145dp"
            android:background="@drawable/square_inner"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/center_view3"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:background="@drawable/square_center"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />



        <View
            android:id="@+id/center_view4"
            android:layout_width="95dp"
            android:layout_height="match_parent"
            android:background="@color/white"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/circle_view00"
            android:layout_width="@dimen/circle_size"
            android:layout_height="@dimen/circle_size"
            android:background="@drawable/circle_white_icon"
            app:layout_constraintBottom_toTopOf="@+id/center_view"
            app:layout_constraintCircle="@id/center_view"
            app:layout_constraintCircleAngle="15"
            app:layout_constraintCircleRadius="@dimen/circle_radius2_square"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/circle_view01"
            android:layout_width="@dimen/circle_size"
            android:layout_height="@dimen/circle_size"
            android:background="@drawable/circle_white_icon"
            app:layout_constraintCircle="@id/center_view"
            app:layout_constraintCircleAngle="45"
            app:layout_constraintCircleRadius="@dimen/circle_radius2_square2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/circle_view02"
            android:layout_width="@dimen/circle_size"
            android:layout_height="@dimen/circle_size"
            android:background="@drawable/circle_white_icon"
            app:layout_constraintCircle="@id/center_view"
            app:layout_constraintCircleAngle="75"
            app:layout_constraintCircleRadius="@dimen/circle_radius2_square"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/circle_view03"
            android:layout_width="@dimen/circle_size"
            android:layout_height="@dimen/circle_size"
            android:background="@drawable/circle_white_icon"
            app:layout_constraintCircle="@id/center_view"
            app:layout_constraintCircleAngle="105"
            app:layout_constraintCircleRadius="@dimen/circle_radius2_square"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/circle_view04"
            android:layout_width="@dimen/circle_size"
            android:layout_height="@dimen/circle_size"
            android:background="@drawable/circle_white_icon"
            app:layout_constraintCircle="@id/center_view"
            app:layout_constraintCircleAngle="135"
            app:layout_constraintCircleRadius="@dimen/circle_radius2_square2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/circle_view05"
            android:layout_width="@dimen/circle_size"
            android:layout_height="@dimen/circle_size"
            android:background="@drawable/circle_white_icon"
            app:layout_constraintCircle="@id/center_view"
            app:layout_constraintCircleAngle="165"
            app:layout_constraintCircleRadius="@dimen/circle_radius2_square"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <!-- inner -->
        <View
            android:id="@+id/circle_view06"
            android:layout_width="@dimen/circle_size"
            android:layout_height="@dimen/circle_size"
            android:background="@drawable/circle_white_icon"
            app:layout_constraintBottom_toTopOf="@+id/center_view"
            app:layout_constraintCircle="@id/center_view"
            app:layout_constraintCircleAngle="15"
            app:layout_constraintCircleRadius="@dimen/circle_radius3_square"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/circle_view07"
            android:layout_width="@dimen/circle_size"
            android:layout_height="@dimen/circle_size"
            android:background="@drawable/circle_white_icon"
            app:layout_constraintCircle="@id/center_view"
            app:layout_constraintCircleAngle="45"
            app:layout_constraintCircleRadius="@dimen/circle_radius3_square2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/circle_view08"
            android:layout_width="@dimen/circle_size"
            android:layout_height="@dimen/circle_size"
            android:background="@drawable/circle_white_icon"
            app:layout_constraintCircle="@id/center_view"
            app:layout_constraintCircleAngle="75"
            app:layout_constraintCircleRadius="@dimen/circle_radius3_square"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/circle_view09"
            android:layout_width="@dimen/circle_size"
            android:layout_height="@dimen/circle_size"
            android:background="@drawable/circle_white_icon"
            app:layout_constraintCircle="@id/center_view"
            app:layout_constraintCircleAngle="105"
            app:layout_constraintCircleRadius="@dimen/circle_radius3_square"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/circle_view10"
            android:layout_width="@dimen/circle_size"
            android:layout_height="@dimen/circle_size"
            android:background="@drawable/circle_white_icon"
            app:layout_constraintCircle="@id/center_view"
            app:layout_constraintCircleAngle="135"
            app:layout_constraintCircleRadius="@dimen/circle_radius3_square2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/circle_view11"
            android:layout_width="@dimen/circle_size"
            android:layout_height="@dimen/circle_size"
            android:background="@drawable/circle_white_icon"
            app:layout_constraintCircle="@id/center_view"
            app:layout_constraintCircleAngle="165"
            app:layout_constraintCircleRadius="@dimen/circle_radius3_square"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>